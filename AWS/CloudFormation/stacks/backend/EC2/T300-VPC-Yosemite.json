{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Launch Yosemite VCP",
  "Resources": {
    "YosemiteVPC": {
      "Type": "AWS::EC2::VPC",
      "Properties": {
        "CidrBlock": "10.0.0.0/16",
        "Tags": [
          {
            "Key": "Application",
            "Value": {
              "Ref": "AWS::StackId"
            }
          }
        ]
      }
    }
  },
  "Outputs": {
    "YosemiteVPCId": {
      "Description": "VPC Launched",
      "Value": {
        "Ref": "YosemiteVPC"
      }
    }
  }
}