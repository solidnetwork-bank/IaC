{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "CI/CD IAM user to interact with AWS",
  "Resources": {
    "AWSCICDAgentUser": {
      "Type" : "AWS::IAM::User",
      "Properties" : {
          "Groups" : [ "CICDAgents" ],  
          "UserName" : "AWSCICDAgent"
        }
    }
  },
  "Outputs": {
    "AWSCICDAgentUserValue": {
      "Description": "AWSCICDAgent User Created in Group CICDAgents.",
      "Value": {
        "Ref": "AWSCICDAgentUser"
      }
    }
  }
}
